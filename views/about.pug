        ul
          li Vanilla JS
          li Node
          li jQuery
          li OOJS
          li Express 
          li Git
          li Code Reviews
        h6 Contact Info

        //- Add your own contact info
        ul
          li sally@thecreativetemp.com

        //- Add URLs to the hrefs below to link to your social media accounts.
        a.button.expanded(href='https://www.linkedin.com/in/sally-poulsen/' target="_blank") LinkedIn
        a.button.expanded(href='https://github.com/kidcompassion' target="_blank") GitHub
        a.button.expanded(href='https://twitter.com/sallypoulsen' target="_blank") Twitter
